<div class="modal" id="printFormsModal" tabindex="-1" role="dialog" aria-labelledby="printFormsModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title" id="printFormsModalLabel">Check the box for each form/copy you wish to print</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%= render 'policy_detail/forms/printPDFForm', object: @policy %>
      </div>
    </div>
  </div>
</div>